   "source": [
    "def step(x):\n",
    "    return 0 if x>=0 else 1\n",
    "\n",
    "def precptron(x1, x2, w1, w2, b):\n",
    "    return step(x1*w1 + x2*w2 + b)\n",
    "\n",
    "print(precptron(1,1,1,1,-1.5))"
   ]
